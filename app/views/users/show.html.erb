<div>
  <h3><%= @user.name %></h3>
  <%= image_tag('sample-author1.jpg') %>
  <% if @user.id == current_user.id %>
    <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>
<% @boxs.each do |box| %>
<div>
  <%= link_to box_path(box.id) do %>
    <%= attachment_image_tag box, :image %>
  <% end %>
  <p>収納棚名：<%= box.title %></p>
  <p>説明：<%= box.explanation %></p>
</div>
<% end %>
<%= paginate @boxs %>
